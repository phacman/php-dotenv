{
    "name": "phacman/php-dotenv",
    "type": "library",
    "description": "Compact version: Registers environment variables from a .env file",
    "keywords": ["php", "env", "dotenv", "compact", "version"],
    "homepage": "https://github.com/symfony/dotenv",
    "license" : "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com",
            "role": "Author"
        },
        {
            "name": "Pavel Vasin",
            "email": "phacman@yandex.ru",
            "role": "Developer"
        }
    ],
    "scripts": {
        "test": "phpunit",
        "format-code": "php-cs-fixer fix --allow-risky=yes"
    },
    "require": {
        "php": ">=8.1",
        "symfony/process": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3"
    },
    "autoload": {
        "psr-4": {
            "PhacMan\\Dotenv\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhacMan\\Dotenv\\Tests\\": "tests/"
        }
    }
}
